    *  

function printPattern(n){
    let pattern ='';
    for(let i=0;i<n;i++){
         for(let j=0; j < n-i-1;j++){
            pattern +=" "
        }
           for(let j=0; j < 2*i+1;j++){
            pattern +="*"
        }
           for(let j=0; j < n-i-1;j++){
            pattern +=" "
        }
        console.log(pattern)
       pattern=''
    }
    for(let i=0;i<n;i++){
        for(let j=0; j < i;j++){
            pattern +=" "
        }
           for(let j=0; j < 2*n - (2*i+1);j++){
            pattern +="*"
        }
           for(let j=0; j < i;j++){
            pattern +=" "
        }
    console.log(pattern)
       pattern=''
    }
}

printPattern(5)
